{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "blocks": [
    {
      "background": "#1A1B2C",
      "black": "#414868",
      "blue": "#7AA2F7",
      "brightBlack": "#414868",
      "brightBlue": "#7AA2F7",
      "brightCyan": "#7DCFFF",
      "brightGreen": "#73DACA",
      "brightPurple": "#BB9AF7",
      "brightRed": "#F7768E",
      "brightWhite": "#C0CAF5",
      "brightYellow": "#E0AF68",
      "cursorColor": "#C0CAF5",
      "cyan": "#7DCFFF",
      "foreground": "#A9B1DC",
      "green": "#73DACA",
      "name": "Tokyo Night",
      "purple": "#BB9AF7",
      "red": "#F7768E",
      "selectionBackground": "#28344A",
      "white": "#C0CAF5",
      "yellow": "#E0AF68"
    },
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#FFEE58",
          "style": "plain",
          "template": "  ",
          "type": "root"
        },
        {
          "background": "#7AA2F7",
          "foreground": "#100e23",
          "powerline_symbol": "",
          "style": "powerline",
          "template": "  {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} ",
          "type": "python"
        },
        {
          "background": "#bb9af7",
          "foreground": "#000000",
          "style": "powerline",
          "powerline_symbol": "",
          "template": " {{ .UserName }}@{{ .HostName }} ",
          "type": "session"
        },
        {
          "background": "#91ddff",
          "foreground": "#100e23",
          "powerline_symbol": "",
          "properties": {
            "style": "full"
          },
          "style": "powerline",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "#95ffa4",
          "foreground": "#193549",
          "powerline_symbol": "",
          "style": "powerline",
          "template": " {{ .HEAD }} ",
          "type": "git"
        },
        {
          "background": "#ff8080",
          "foreground": "#ffffff",
          "powerline_symbol": "",
          "style": "powerline",
          "template": "  {{ .Code }} ",
          "type": "status"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true
}
