                "action": "resizePane",
                "direction": "down"
            },
            "keys": "ctrl+alt+shift+minus"
        },
        {
            "command": "scrollDownPage",
            "keys": "ctrl+shift+pgdn"
        },
        {
            "command": "scrollUpPage",
            "keys": "ctrl+shift+pgup"
        },
        {
            "command": "scrollDown",
            "keys": "ctrl+shift+j"
        },
        {
            "command": "scrollUp",
            "keys": "ctrl+shift+k"
        },
        {
            "command": "find",
            "keys": "ctrl+shift+f"
        },
        {
            "command": {
                "action": "splitPane",
                "split": "auto",
                "splitMode": "duplicate"
            },
            "keys": "ctrl+shift+\\"
        },
        {
            "command": "toggleSplitOrientation",
            "keys": "ctrl+alt+shift+\\"
        },
        {
            "command": {
                "action": "openSettings",
                "target": "defaultsFile"
            },
            "keys": "ctrl+alt+shift+comma"
        },
        {
            "command": {
                "action": "moveFocus",
                "direction": "left"
            },
            "keys": "ctrl+alt+h"
        },
        {
            "command": {
                "action": "moveFocus",
                "direction": "right"
            },
            "keys": "ctrl+alt+l"
        },
        {
            "command": {
                "action": "moveFocus",
                "direction": "up"
            },
            "keys": "ctrl+alt+k"
        },
        {
            "command": {
                "action": "moveFocus",
                "direction": "down"
            },
            "keys": "ctrl+alt+j"
        },
        {
            "command": {
                "action": "moveFocus",
                "direction": "nextInOrder"
            },
            "keys": "alt+`"
        },
        {
            "command": {
                "action": "clearBuffer",
                "clear": "all"
            },
            "keys": "ctrl+shift+l"
        },
        {
            "command": "togglePaneZoom",
            "keys": "ctrl+shift+enter"
        },
        {
            "command": "toggleFocusMode",
            "keys": "ctrl+shift+z"
        }
    ],
    "centerOnLaunch": true,
    "confirmCloseAllTabs": false,
