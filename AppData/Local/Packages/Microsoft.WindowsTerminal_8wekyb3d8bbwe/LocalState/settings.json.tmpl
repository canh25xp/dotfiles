            "id": "Terminal.ToggleFocusMode",
            "keys": "ctrl+shift+z"
        },
        {
            "id": null,
            "keys": "ctrl+v"
        },
        {
            "id": "Terminal.FindText",
            "keys": "ctrl+shift+f"
        },
        {
            "id": null,
            "keys": "alt+up"
        },
        {
            "id": null,
            "keys": "alt+right"
        },
        {
            "id": "Terminal.ScrollUp",
            "keys": "ctrl+shift+k"
        },
        {
            "id": null,
            "keys": "ctrl+c"
