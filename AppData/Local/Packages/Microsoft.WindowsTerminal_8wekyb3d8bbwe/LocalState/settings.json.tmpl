{
    "$help": "https://aka.ms/terminal-documentation",
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "actions": [
        {
            "command": "unbound",
            "keys": "ctrl+v"
        },
        {
            "command": {
                "action": "copy",
                "singleLine": false
            },
            "keys": "ctrl+c"
        },
        {
            "command": {
                "action": "resizePane",
                "direction": "right"
            },
            "keys": "ctrl+shift+plus"
        },
        {
            "command": {
                "action": "resizePane",
                "direction": "left"
            },
            "keys": "ctrl+shift+minus"
        },
        {
            "command": {
                "action": "resizePane",
                "direction": "up"
            },
            "keys": "ctrl+alt+shift+plus"
        },
        {
            "command": {
                "action": "resizePane",
                "direction": "down"
            },
            "keys": "ctrl+alt+shift+minus"
        },
        {
            "command": "scrollDownPage",
            "keys": "ctrl+shift+pgdn"
        },
        {
            "command": "scrollUpPage",
            "keys": "ctrl+shift+pgup"
        },
        {
            "command": "scrollDown",
            "keys": "ctrl+shift+j"
        },
        {
            "command": "scrollUp",
            "keys": "ctrl+shift+k"
        },
        {
            "command": "find",
            "keys": "ctrl+shift+f"
        },
        {
            "command": {
                "action": "splitPane",
                "split": "auto",
                "splitMode": "duplicate"
            },
            "keys": "ctrl+shift+\\"
        },
        {
            "command": "toggleSplitOrientation",
            "keys": "ctrl+alt+shift+\\"
        },
        {
            "command": {
                "action": "openSettings",
                "target": "defaultsFile"
            },
            "keys": "ctrl+alt+shift+comma"
        },
        {
            "command": {
                "action": "moveFocus",
                "direction": "left"
            },
            "keys": "ctrl+alt+h"
        },
        {
            "command": {
                "action": "moveFocus",
                "direction": "right"
            },
            "keys": "ctrl+alt+l"
        },
        {
            "command": {
                "action": "moveFocus",
                "direction": "up"
            },
            "keys": "ctrl+alt+k"
        },
        {
            "command": {
                "action": "moveFocus",
                "direction": "down"
            },
            "keys": "ctrl+alt+j"
        },
        {
            "command": {
                "action": "moveFocus",
                "direction": "nextInOrder"
            },
            "keys": "alt+`"
        },
        {
            "command": {
                "action": "clearBuffer",
                "clear": "all"
            },
            "keys": "ctrl+shift+l"
        },
        {
            "command": "togglePaneZoom",
            "keys": "ctrl+shift+enter"
        },
        {
            "command": "toggleFocusMode",
            "keys": "ctrl+shift+z"
        }
    ],
    "centerOnLaunch": true,
    "confirmCloseAllTabs": false,
    "initialRows": 40,
    "launchMode": "maximized",
    "copyFormatting": "none",
    "copyOnSelect": true,
    "disableAnimations": true,
    "defaultProfile": {{ if .work -}} "{61c54bbd-c2c6-5271-96e7-009a87ff44bf}" {{- else -}} "{a3a2e83a-884a-5379-baa8-16f193a13b21}" {{- end }},
    "newTabMenu": [
        {
            "type": "remainingProfiles"
        }
    ],
    "profiles": {
        "defaults": {
            "backgroundImage": "%USERPROFILE%/Pictures/Wallpapers/YorForger.jpg",
            "backgroundImageOpacity": 0.15,
            "closeOnExit": "always",
            "colorScheme": {
                "dark": "Catppuccin Mocha",
                "light": "Catppuccin Latte"
            },
            "cursorShape": "filledBox",
            "font": {
                "cellHeight": "1.0",
                "face": "CaskaydiaCove Nerd Font",
                "features": {
                    "calt": 0
                },
                "size": {{ .fontSize }}
            },
            "padding": "0",
            "scrollbarState": "hidden"
        },
        "list": [
