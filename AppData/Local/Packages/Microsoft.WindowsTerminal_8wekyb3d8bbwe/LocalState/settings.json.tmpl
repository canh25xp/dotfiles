{
    "$help": "https://aka.ms/terminal-documentation",
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "centerOnLaunch": true,
    "confirmCloseAllTabs": false,
    "initialRows": 40,
    "launchMode": "maximized",
    "copyFormatting": "none",
    "copyOnSelect": true,
    "disableAnimations": false,
    "actions": [
        { "command": "unbound", "keys": "ctrl+v" },
        { "command": "unbound", "keys": "ctrl+c" },
        { "command": "unbound", "keys": "alt+left" },
        { "command": "unbound", "keys": "alt+right" },
        { "command": "unbound", "keys": "alt+up" },
        { "command": "unbound", "keys": "alt+down" },

        { "command": "find",    "keys": "ctrl+shift+f" },

        { "command": { "action": "resizePane", "direction": "left" },   "keys": "ctrl+shift+minus" },
        { "command": { "action": "resizePane", "direction": "right" },  "keys": "ctrl+shift+plus" },
        { "command": { "action": "resizePane", "direction": "up" },     "keys": "ctrl+alt+shift+plus" },
        { "command": { "action": "resizePane", "direction": "down" },   "keys": "ctrl+alt+shift+minus" },

        { "command": { "action": "moveFocus", "direction": "left" },        "keys": "ctrl+alt+h" },
        { "command": { "action": "moveFocus", "direction": "right" },       "keys": "ctrl+alt+l" },
        { "command": { "action": "moveFocus", "direction": "up" },          "keys": "ctrl+alt+k" },
        { "command": { "action": "moveFocus", "direction": "down" },        "keys": "ctrl+alt+j" },
        { "command": { "action": "moveFocus", "direction": "nextInOrder" }, "keys": "alt+`" },

        { "command": "scrollUp",        "keys": "ctrl+shift+k" },
        { "command": "scrollDown",      "keys": "ctrl+shift+j" },
        { "command": "scrollUpPage",    "keys": "ctrl+shift+pgup" },
        { "command": "scrollDownPage",  "keys": "ctrl+shift+pgdn" },

        { "command": { "action": "splitPane", "split": "auto", "splitMode": "duplicate" },  "keys": "ctrl+shift+\\" },
        { "command": "toggleSplitOrientation",                                              "keys": "ctrl+alt+shift+\\" },

        { "command": "togglePaneZoom",  "keys": "ctrl+shift+enter" },
        { "command": "toggleFocusMode", "keys": "ctrl+shift+z" },

        { "command": { "action": "openSettings", "target": "defaultsFile" }, "keys": "ctrl+alt+shift+comma" },
        { "command": { "action": "clearBuffer", "clear": "all" }, "keys": "ctrl+shift+l" }
    ],
    "defaultProfile": {{ if .work -}} "{61c54bbd-c2c6-5271-96e7-009a87ff44bf}" {{- else -}} "{a3a2e83a-884a-5379-baa8-16f193a13b21}" {{- end }},
