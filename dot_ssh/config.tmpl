{{ if .work -}}
IdentityFile ~/.ssh/id_ecdsa_work
{{- else if .personal -}}
IdentityFile ~/.ssh/id_ecdsa
{{- end }}

Host desktop
  HostName 192.168.0.100
  User Michael
  IdentityFile ~/.ssh/id_ecdsa
  KexAlgorithms sntrup761x25519-sha512@openssh.com,curve25519-sha256,curve25519-sha256@libssh.org,ecdh-sha2-nistp256

Host laptop
  HostName 192.168.0.101
  User Michael
  IdentityFile ~/.ssh/id_ecdsa

Host phone
  HostName 192.168.0.102
  User u0_a453
  Port 8022
  IdentityFile ~/.ssh/id_ecdsa

Host tablet
  HostName localhost
  User u0_a353
  Port 8022
  IdentityFile ~/.ssh/id_ecdsa_work

Host wifiteam
  HostName 192.168.23.13
  User "vancanh.ng"
  IdentityFile ~/.ssh/id_ecdsa_work

Host desktop-jump
  HostName 100.99.0.0
  User Michael
  IdentityFile ~/.ssh/id_ecdsa_work
  KexAlgorithms sntrup761x25519-sha512@openssh.com,curve25519-sha256,curve25519-sha256@libssh.org,ecdh-sha2-nistp256
  ProxyJump tablet

Host wifiteam-jump
  HostName 192.168.23.13
  User "vancanh.ng"
  IdentityFile ~/.ssh/id_ecdsa_work
  ProxyJump tablet

Host github.com
  HostName github.com
  PreferredAuthentications publickey
  IdentityFile ~/.ssh/id_rsa_gh

Host gitlab.com
  HostName gitlab.com
  PreferredAuthentications publickey
  IdentityFile ~/.ssh/id_rsa_gl
